package Tools;


/**
 * Write a description of class Zoom here.
 * 
 * @author (your name) 
 * @version (a version number or a date)
 */
public class ZoomModel
{
    private int zoomFactor =0;
    

    public void setZoomFactor(int zf)
    {
        zoomFactor =zf;
    }
    
    public int toScreen(double wc)
    {
        int scor = (int)Math.round(wc/zoomFactor);
        return scor;
    }
}
