package Tools;


import java.awt.*;
public class EraserHandler extends AbstractHandler
{
    public   void handle(int x,int y, int x1, int y1)
   {
       int w = 5;
       int h = 5;
       Easel h = AbstractHandler.easel;
       for(int i=0;i<25;i++)
       {
           h.image.setRGB(h.image.getRGB().darker());
       }
   }
}
